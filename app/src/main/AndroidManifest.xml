<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="mmjp.fsm.ford.com.mmjp">

<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
<uses-permission android:name="android.permission.INTERNET" />
<uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

<application
    android:name=".app.MainActivity"
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher_mmjp"
    android:label="@string/appName"
    android:largeHeap="true"
    android:roundIcon="@mipmap/ic_launcher_mmjp"
    android:supportsRtl="true"
    android:theme="@style/AppTheme">
    <activity
        android:name=".app.profile.splash.SplashActivity"
        android:screenOrientation="portrait"
        android:theme="@style/AppTheme.NoActionBar">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
    <activity
        android:name=".app.profile.home.HomeView"
        android:theme="@style/AppTheme.NoActionBar" />
    <activity
        android:name=".app.profile.login.LoginView"
        android:theme="@style/AppTheme.WhiteActionBar" />
    <activity
        android:name=".app.profile.forgetpasscode.ForgetPasscodeView"
        android:theme="@style/AppTheme.WhiteActionBar" />
    <activity
        android:name=".app.profile.splash.AnimActivity"
        android:screenOrientation="portrait"
        android:theme="@style/AppTheme.NoActionBar" />
    <activity
        android:name=".app.trip.modules.origin.OriginView"
        android:label="@string/title_plan_trip"
        android:theme="@style/AppTheme.WhiteActionBar" />
    <activity
        android:name=".app.trip.modules.route.RouteSearchActivity"
        android:label="@string/title_route_search"
        android:theme="@style/AppTheme.WhiteActionBar" />
    <activity
        android:name=".app.trip.modules.destination.DestinationView"
        android:label="@string/title_activity_home"
        android:theme="@style/AppTheme.WhiteActionBar" />
    <activity
        android:name=".app.profile.registration.RegistrationView"
        android:screenOrientation="portrait"
        android:theme="@style/AppTheme.WhiteActionBar"
        android:windowSoftInputMode="stateHidden" />
    <activity
        android:name=".app.trip.modules.summary.RouteSummaryActivity"
        android:label="@string/title_activity_route_summary"
        android:theme="@style/AppTheme.NoActionBar"/>
    <activity
        android:name=".app.profile.registration.accounts.AccountLinkingView"
        android:label="@string/title_activity_route_summary"
        android:theme="@style/AppTheme.NoActionBar"/>
    <activity
        android:name=".app.route.providers.uber.modules.rider.UberRiderView"
        android:label="@string/title_activity_route_summary"
        android:theme="@style/AppTheme.NoActionBar"/>
    <activity
        android:name=".app.route.providers.lyft.rider.LyftRiderView"
        android:label="@string/title_activity_route_summary"
        android:theme="@style/AppTheme.NoActionBar"/>

    <!--
   Developers should put application credentials here.To obtain them, please register the application
   at https://developer.here.com/develop/mobile-sdks
